{
	"name": "uwuv2-commands",
	"displayName": "uwubotv2-commands",
	"publisher": "ackhack",
	"description": "",
	"version": "2.0.0",
	"engines": {
		"vscode": "^1.44.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:uwu-ct.createDC",
		"onCommand:uwu-ct.DCtoNC",
		"onCommand:uwu-ct.DCtoAC"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "uwu-ct.createDC",
				"title": "uwuBot: Create Developer Command Template"
			},
			{
				"command": "uwu-ct.DCtoNC",
				"title": "uwuBot: Convert Developer Command to Normal Command"
			},
			{
				"command": "uwu-ct.DCtoAC",
				"title": "uwuBot: Convert Developer Command to Admin Command"
			}
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.44.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.1",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"fs": "0.0.1-security"
	}
